{
    "name": "slothsoft/savegame",
    "description": "A Farah module for editing arbitrary savegame files.",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Daniel Schulz",
            "email": "info.slothsoft@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": "^7.2",
        "php-ds/php-ds": "^1.2",
        "slothsoft/core": "dev-develop",
        "slothsoft/farah": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "suggest": {
        "ext-ds": "*"
    },
    "autoload": {
        "psr-0": {
            "Slothsoft\\Savegame\\": "src/"
        },
        "files": [
            "src/registerModule.php"
        ]
    },
    "keywords": [
        "slothsoft",
        "farah-module",
        "docs"
    ],
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "classmap-authoritative": false,
    "minimum-stability": "dev",
    "prefer-stable": true
}
